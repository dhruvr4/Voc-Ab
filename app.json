{
  "expo": {
    "name": "VocAB",
    "slug": "VocAB",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.NerdDevz.vocabulary",
      "supportsTablet": true
    },
    "android":{
      "package":"com.NerdDevz.vocabulary",
      "versionCode": 13 // Notice the versionCode added under android.
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
